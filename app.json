{
  "expo": {
    "icon": "./assets/icon.png",
    "name": "Una Astro-Tarot AI",
    "slug": "tarot-mobile",
    "owner": "mare82",
    "version": "1.7.0",
    "orientation": "portrait",
    "sdkVersion": "53.0.0",
    "scheme": "com.mare82.tarotmobile",
    "extra": {
      "eas": {
        "projectId": "1c8251b9-7c40-4b86-92b5-96612afcc36b"
      }
    },
    "android": {
      "package": "com.mare82.tarotmobile",
      "versionCode": 7,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "com.android.vending.BILLING"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "com.mare82.tarotmobile"
            }
          ]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.mare82.tarotmobile",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "We use your device identifier to show relevant ads (AdMob) and support the development of this app."
      }
    },
    "plugins": [
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2786609619751533~1407821486",
          "iosAppId": "ca-app-pub-2786609619751533~5646433485"
        }
      ],
      "expo-notifications",
      "expo-font",
      "expo-web-browser",
      "expo-audio",
      "expo-tracking-transparency",
      "expo-iap"
    ],
    "splash": {
      "image": "./assets/una.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B1026"
    }
  }
}

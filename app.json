{
  "expo": {
    "icon": "./assets/icon.png",
    "name": "Una AI Tarot: Jung",
    "slug": "tarot-mobile",
    "owner": "mare82",
    "version": "2.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "scheme": "com.mare82.tarotmobile",
    "extra": {
      "eas": {
        "projectId": "1c8251b9-7c40-4b86-92b5-96612afcc36b"
      },
      "__legacyATT": {
        "NSUserTrackingUsageDescription": "We use your device identifier to show relevant ads (AdMob) and support the development of this app.",
        "plugin": "expo-tracking-transparency"
      }
    },
    "android": {
      "package": "com.mare82.unatarot",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "com.android.vending.BILLING",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "com.google.android.gms.permission.AD_ID"
      ],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "com.mare82.tarotmobile"
            }
          ]
        }
      ],
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "ios": {
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.mare82.unatarot",
      "buildNumber": "1",
      "supportsTablet": false,
      "requireFullScreen": true,
      "usesAppleSignIn": true,
      "infoPlist": {
        "UIRequiredDeviceCapabilities": [
          "telephony"
        ],
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "targetSdkVersion": 35,
            "compileSdkVersion": 35
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2786609619751533~4025394892",
          "iosAppId": "ca-app-pub-2786609619751533~4085104640"
        }
      ],
      "expo-notifications",
      "expo-font",
      "expo-web-browser",
      "expo-audio",
      "expo-iap",
      "expo-localization"
    ],
    "splash": {
      "image": "./assets/una.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B1026"
    },
    "updates": {
      "url": "https://u.expo.dev/1c8251b9-7c40-4b86-92b5-96612afcc36b"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
